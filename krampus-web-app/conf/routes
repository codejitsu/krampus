
# Home page
GET     /stream/:channel   controllers.WikiwatchController.stream(channel: String)
GET     /wikiwatch         controllers.WikiwatchController.wikiwatch
GET     /wiki              controllers.WikiwatchController.wiki

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file      controllers.Assets.at(path="/public", file)
GET     /webjars/*file     controllers.WebJarAssets.at(file)